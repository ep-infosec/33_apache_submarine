      "replicas": 1,
      "resources": "cpu=1,memory=512M"
    },
    "Worker": {
      "replicas": 1,
      "resources": "cpu=1,memory=512M"
    }
  },
  "code": {
    "syncMode": "git",
    "url" : "https://github.com/apache/submarine.git"
  }
}
